        android:textSize="19sp"
        android:layout_marginTop="7dp"
        android:textColor="@color/azul_escuro"
        android:layout_marginStart="290dp">
    </TextView>


    <CheckBox
        android:id="@+id/cb_brinquedos"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginStart="15dp"
        android:layout_marginTop="9dp"
        android:background="@drawable/border_servicos"
        android:layout_below="@id/tv_parquinho">
    </CheckBox>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/brinquedos"
        android:id="@+id/tv_brinquedos"
        android:layout_below="@id/tv_parquinho"
        android:textSize="25sp"
        android:textColor="@color/azul_escuro"
        android:layout_marginStart="60dp">
    </TextView>


    <CheckBox
        android:id="@+id/cb_spa"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginStart="240dp"
        android:layout_marginTop="9dp"
        android:background="@drawable/border_servicos"
        android:layout_below="@id/tv_parquinho">
    </CheckBox>
    <TextView
