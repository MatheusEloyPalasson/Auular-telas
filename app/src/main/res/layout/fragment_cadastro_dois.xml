<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/_1_2_3"
        android:textSize="40sp"
        android:textColor="#000000"
        android:layout_centerHorizontal="true"
        android:id="@+id/tv_indicador">
    </TextView>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="10dp"
        android:orientation="horizontal"
        android:layout_below="@id/tv_indicador"
        android:layout_centerHorizontal="true"
        android:id="@+id/ll_pagina">

        <EditText
            android:layout_width="30dp"
            android:layout_height="10dp"
            android:textSize="20dp"
            android:background="@drawable/border_yellow"
            android:id="@+id/et_pagina_um">
        </EditText>

        <EditText
            android:layout_width="30dp"
            android:layout_height="10dp"
            android:textSize="20sp"
            android:background="#FFFFFF">
        </EditText>

        <EditText
            android:layout_width="30dp"
            android:layout_height="10dp"
            android:textSize="20sp"
            android:background="@drawable/border_blue"
            android:id="@+id/et_pagina_dois">
        </EditText>

        <EditText
            android:layout_width="30dp"
            android:layout_height="10dp"
            android:textSize="20sp"
            android:background="#FFFFFF">
        </EditText>

        <EditText
            android:layout_width="30dp"
            android:layout_height="10dp"
            android:textSize="20sp"
            android:background="@drawable/border_yellow"
            android:id="@+id/et_pagina_tres">
        </EditText>


    </LinearLayout>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/amarelo_padrao"
        android:text="@string/empresa"
        android:layout_below="@+id/ll_pagina"
        android:layout_centerHorizontal="true"
        android:textSize="30sp"
        android:id="@+id/tv_empresa">
    </TextView>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/ocupa_o"
        android:textColor="@color/azul_escuro"
        android:layout_below="@id/tv_empresa"
        android:textSize="25sp"
        android:id="@+id/tv_ocupacao"
        android:layout_marginStart="10dp">
    </TextView>

    <EditText
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:layout_below="@id/tv_ocupacao"
        android:text="@string/_35"
        android:textColor="@color/azul_escuro"
        android:background="@drawable/border"
        android:id="@+id/et_ocupacao">
    </EditText>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/servi_os"
        android:textColor="@color/azul_escuro"
        android:layout_below="@id/et_ocupacao"
        android:textSize="25sp"
        android:id="@+id/tv_servicos"
        android:layout_marginStart="10dp">
    </TextView>


    <CheckBox
        android:id="@+id/cb_piscina"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginStart="15dp"
        android:layout_marginTop="9dp"
        android:background="@drawable/border_servicos"
        android:layout_below="@id/tv_servicos">
    </CheckBox>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/piscina"
        android:id="@+id/tv_piscina"
        android:layout_below="@id/tv_servicos"
        android:textSize="25sp"
        android:textColor="@color/azul_escuro"
        android:layout_marginStart="60dp">
    </TextView>

    <CheckBox
        android:id="@+id/cb_passeio"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginStart="240dp"
        android:layout_marginTop="9dp"
        android:background="@drawable/border_servicos"
        android:layout_below="@id/tv_servicos">
    </CheckBox>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/passeio"
        android:id="@+id/tv_passeio"
        android:layout_below="@id/tv_servicos"
        android:textSize="25sp"
        android:textColor="@color/azul_escuro"
        android:layout_marginStart="290dp">
    </TextView>


    <CheckBox
        android:id="@+id/cb_parquinho"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginStart="15dp"
        android:layout_marginTop="9dp"
        android:background="@drawable/border_servicos"
        android:layout_below="@id/tv_piscina">
    </CheckBox>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/parquinho"
        android:id="@+id/tv_parquinho"
        android:layout_below="@id/tv_piscina"
        android:textSize="25sp"
        android:textColor="@color/azul_escuro"
