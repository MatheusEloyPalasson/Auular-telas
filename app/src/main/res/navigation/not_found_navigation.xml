<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/not_found_navigation"
    app:startDestination="@id/errorActivity">

    <activity
        android:id="@+id/mainActivity"
        android:name="com.example.auular.utils.MainActivity"
        android:label="activity_main"
        tools:layout="@layout/activity_main" />
    <navigation android:id="@+id/navigation"
        app:startDestination="@id/errorActivity">
        <activity
            android:id="@+id/errorActivity"
            android:name="com.example.auular.ErrorActivity"
            android:label="activity_error"
            tools:layout="@layout/activity_error" />
    </navigation>
</navigation>